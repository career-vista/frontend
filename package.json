{
  "name": "careervista-ai-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://career-vista.github.io/frontend",
  "scripts": {
    "dev": "vite",
    "dev:env": "powershell -ExecutionPolicy Bypass -File ./dev-start.ps1",
    "build": "tsc && vite build",
    "build:gh-pages": "tsc && vite build --base=/frontend/",
    "deploy": "pnpm run build:gh-pages && gh-pages -d dist",
    "predeploy": "pnpm run build:gh-pages",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/lab": "7.0.1-beta.18",
    "@mui/material": "^7.3.2",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.2",
    "firebase": "^10.6.0",
    "googleapis": "^164.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-toastify": "^9.1.3",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "cypress": "^13.6.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0"
  }
}
